{
  "name": "gnu-poke",
  "displayName": "GNU poke",
  "description": "Language support for GNU poke",
  "version": "0.0.1",
  "publisher": "conlan-wesson",
  "license": "GPL-3.0+",
  "icon": "./images/gnu-poke.png",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Programming Languages", "Snippets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cwesson/vscode-gnu-poke"
  },
  "contributes": {
    "languages": [{
      "id": "poke",
      "aliases": ["Poke", "poke", "GNU poke"],
      "extensions": [".pk", ".map"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "poke",
      "scopeName": "source.pk",
      "path": "./syntaxes/poke.tmLanguage.json"
    }],
    "snippets": [{
        "language": "poke",
        "path": "./snippets/snippets.json"
    }]
  }
}
